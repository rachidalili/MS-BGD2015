     "input": [
      "def cleanTheData_andGetJson(text): #the output is a table of jsons that does not contain empty projects\n",
      "    #text = text.replace(\"u'\", '\\'').replace('\\'', '\"')\n",
      "    myJson = json.loads(text)\n",
      "    for proposition in myJson:\n",
      "        emptyItems = []\n",
      "        for project in proposition:\n",
      "            if len(proposition[project]) == 0:\n",
      "                emptyItems.append(project)\n",
      "        for emptyItem in emptyItems:\n",
      "            proposition.pop(emptyItem)\n",
      "    return myJson\n",
      "\n",
      "def projectSatisfiesAffinity(project, affinity): #check if the project contains this affinity\n",
      "    satisfied = True\n",
      "    for person in affinity:\n",
      "        if not person in project:\n",
      "       #     print person + \" is not in \" + str(project)\n",
      "            satisfied = False\n",
      "       # else:\n",
      "       #     print person + \" is in \" + str(project)\n",
      "            \n",
      "    return satisfied\n",
      "    \n",
      "def getScoreForProposition(proposition, affinities): #get score for proposition, based on how many affinities it does satisfy\n",
      "    score = 0\n",
      "    for project in proposition:\n",
      "        #print \"project \" + project\n",
      "        for affinity in affinities:\n",
      "            #print \"affinity \" + str(affinity)\n",
      "            if projectSatisfiesAffinity(proposition[project], affinity):\n",
      "                score = score + 1\n",
      "    return score\n"
     ],
